<mvc:View
    controllerName="namespace.studies.controller.ODataModel"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Page title="OData Model">
        <List items="{/Products}">
            <headerToolbar>
                <Toolbar>
                    <Title text="Northwind Data" />
                    <ToolbarSpacer />
                    <Button text="Create" press="openCreateDialog"/>
                </Toolbar>
            </headerToolbar>
            <items>
                <StandardListItem title="ID: {ID}" info="Rating: {Rating}"/>
            </items>
        </List>
    </Page>
</mvc:View>
